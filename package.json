{
  "name": "react-workers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:react": "react-scripts start",
    "start:workers": "npm run build:worker -- --watch",
    "start:server": "node ./server/index.js",
    "start": "run-p start:server start:workers start:react",
    "build:worker": "webpack --config worker/webpack.config.js",
    "build:react": "react-scripts build",
    "build": "num run build:worker && npm run build:react",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@d3fc/d3fc-chart": "^2.6.2",
    "@d3fc/d3fc-element": "^5.2.3",
    "@d3fc/d3fc-extent": "^3.1.4",
    "@d3fc/d3fc-random-data": "^3.0.8",
    "@d3fc/d3fc-rebind": "^5.0.7",
    "@d3fc/d3fc-series": "^4.0.18",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-time": "^1.0.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.3.4"
  }
}
